msgctxt "#600"
msgid "@DISTRONAME@ Settings"
msgstr "Налаштування @DISTRONAME@"

msgctxt "#601"
msgid "Interface"
msgstr "Інтерфейс"

msgctxt "#602"
msgid "Address"
msgstr "Адреса"

